# Environment: production, development, or test
ENVIRONMENT=production

# PostgreSQL password (used in docker-compose)
POSTGRES_PASSWORD=websearch_password

# Admin credentials
ADMIN_USERNAME=admin
ADMIN_PASSWORD=your-secure-password
ADMIN_SESSION_SECRET=your-session-secret

# API Keys
INDEXER_API_KEY=your-indexer-api-key
OPENAI_API_KEY=your-openai-api-key

# Security
ALLOWED_HOSTS=palebluesearch.com,www.palebluesearch.com
